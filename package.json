  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/bun": "latest",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "2.1.4",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.14.0",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "typescript-eslint": "^8.14.0",
    "vitest": "^2.1.4",
